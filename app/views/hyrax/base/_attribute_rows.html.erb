<%# @todo - labels, renderers, see base/_attribute_rows %>

<% @admin_set_id ||= AdminSet.where(title: presenter.admin_set).first.id
dynamic_schema_service = dynamic_schema_helper(@admin_set_id,
presenter.model.to_s,
presenter.dynamic_schema )%>

<% dynamic_schema_service.view_properties.each_pair do | prop_key, prop_value | %>
  <%# @todo internationalisation %>
  <%= presenter.attribute_to_html(prop_key, label: prop_value[:label], html_dl: true) %>
<% end %>
<% if @presenter.respond_to? :holding_location %>
  <%= @presenter.holding_location %>
<% end %>
